<?xml version="1.0"?>
<syntax name="Debian Control">
	<single>
		<!--Elements-->
		<group>
			<format color="#009933" weight="75" />
			<pattern>
				<start>\b</start>
				<end>\b</end>
			</pattern>
			<item>Package</item>
			<item>Source</item>
			<item>Version</item>
			<item>Section</item>
			<item>Priority</item>
			<item>Architecture</item>
			<item>Essential</item>
			<item>Depends</item>
			<item>Installed-Size</item>
			<item>Maintainer</item>
			<item>Description</item>
			<item>Homepage</item>
		</group>
		<!--Comments-->
		<group>
			<format color="#0066ff" weight="75" />
			<pattern>
				<start></start>
				<end>[^\n]*</end>
			</pattern>
			<item>#</item>
		</group>
	</single>
</syntax> 
